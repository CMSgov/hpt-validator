{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "standard_charges": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "payers_information": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "estimated_amount": {
                  "type": "number",
                  "not": {
                    "const": 999999999,
                    "$message": "Nine 9s used for estimated amount."
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
